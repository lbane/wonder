echo "Fetch upstream"
git fetch upstream || exit 1
echo "Checkout master"
git checkout master || exit 1
echo "Merge upstream/master"
git merge upstream/master || exit 1
echo "Checkout working
git checkout working || exit 1
echo "Merge master"
git merge master || exit 1
echo "Build Frameworks"
ant clean frameworks; sudo ant -Dwo.external.root=/Users/ralf/Roots -Duser.home=$HOME frameworks.install || exit 1
echo "Build applications"
ant applications; sudo ant -Dwo.external.root=/Users/ralf/Roots -Duser.home=$HOME applications.install || exit 1
echo "Build examples"
ant examples; sudo ant -Dwo.external.root=/Users/ralf/Roots -Duser.home=$HOME examples.install || exit 1

